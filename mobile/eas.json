{
  "cli": {
    "version": ">= 15.0.10",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "dev",
        "APP_DOMAIN": "api.preview.friendspot.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview",
        "APP_DOMAIN": "api.preview.friendspot.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      },
      "channel": "production",
      "env": {
        "APP_DOMAIN": "api.friendspot.app"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6742799884"
      }
    }
  }
}
